# Exemplo de arquivo de secrets para Streamlit Cloud
# Copie este arquivo para secrets.toml e preencha com seus valores reais
# NUNCA commite o arquivo secrets.toml no Git!

TRANSPARENCIA_API_TOKEN = "seu_token_aqui"
TRANSPARENCIA_EMAIL = "seu_email@exemplo.com"

# Outras configurações opcionais
[database]
host = "localhost"
port = 5432
database = "transparencia_db"
user = "user"
password = "password"

[api]
rate_limit = 30
timeout = 30